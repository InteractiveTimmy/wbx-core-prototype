{
  "name": "webex-core-prototype",
  "version": "0.0.0",
  "description": "Webex Core Prototype",
  "main": "index.js",
  "repository": "git@github.com:InteractiveTimmy/webex-core-prototype.git",
  "author": "Timothy Scheuering <timsch@cisco.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean:dist && yarn build:loose && yarn build:bundle",
    "build:bundle": "rollup -c ./config/rollup.config.js",
    "build:clean": "rimraf ./dist",
    "build:loose": "tsc",
    "clean": "yarn clean:dist && yarn clean:docs",
    "clean:dist": "rimraf ./dist",
    "clean:docs": "rimraf ./docs",
    "docs": "yarn clean:docs && yarn docs:extract && yarn docs:generate",
    "docs:extract": "api-extractor run -c ./config/api-extractor.config.json",
    "docs:generate": "api-documenter markdown -i ./docs/loose -o ./docs",
    "prepare": "husky install config/husky",
    "start": "echo \"NOTE: this project is meant to be consumed by other projects only.\"",
    "test": "yarn test:static && yarn test:unit && yarn test:syntax && yarn test:integration && yarn test:performance",
    "test:integration": "echo \"TODO: this script has not been implemented\"",
    "test:performance": "echo \"TODO: this script has not been implemented\"",
    "test:static": "eslint ./src/**",
    "test:syntax": "tsc --noEmit",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@microsoft/api-documenter": "^7.14.1",
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.25.0"
  }
}
